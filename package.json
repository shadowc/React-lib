{
  "name": "react-lib",
  "version": "1.3.5",
  "description": "A set of React components mapped to JS classes for ease of use",
  "main": "dist/ReactLib.js",
  "scripts": {
    "test": "./node_modules/.bin/jest tests/*.*",
    "lint": "./node_modules/.bin/eslint src/**/*.*",
    "docs": "node ./build/docs.js",
    "build": "./node_modules/.bin/webpack",
    "prerelease": "npm run test",
    "release": "npm run lint && npm run docs",
    "postrelease": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git@matiasjose.com:/home/git/gitrepositories/react-lib.git"
  },
  "keywords": [
    "reactjs",
    "react-dom"
  ],
  "author": "Matias Jose",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-jest": "^22.1.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "clean-webpack-plugin": "^0.1.17",
    "eslint": "^4.16.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.5.1",
    "jest": "^29.7.0",
    "jquery": "^3.3.1",
    "jsdoc-to-markdown": "^8.0.0",
    "prop-types": "^15.8.1",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-test-renderer": "^16.14.0",
    "uglifyjs-webpack-plugin": "^1.1.6",
    "webpack": "^3.10.0"
  },
  "jest": {
    "verbose": true,
    "transform": {
      "^.+\\.js$": "babel-jest",
      "^.+\\.jsx$": "babel-jest"
    },
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleDirectories": [
      "node_modules",
      "src"
    ]
  }
}
